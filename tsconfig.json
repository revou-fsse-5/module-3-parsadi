{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist"
    },
    "include": [
        "src/*/.ts",
        "index.ts" //ngebaca file ts
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}